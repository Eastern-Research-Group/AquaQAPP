{
  "name": "aquaqapp",
  "scripts": {
    "build": "npm run migrate && npm run seed && vue-cli-service build",
    "prod": "node server/src/server.js",
    "lint": "eslint .",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "server": "npm run migrate && npm run seed && nodemon --ignore 'client/*' --inspect server/src/server.js",
    "client": "vue-cli-service serve",
    "start": "concurrently --kill-others -n server,client \"npm run server\" \"npm run client\"",
    "test": "vue-cli-service test:unit"
  },
  "engines": {
    "node": "10.15.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.0.13",
    "@websanova/vue-auth": "^2.21.14-beta",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.0",
    "leaflet": "^1.4.0",
    "nodemailer": "^6.1.0",
    "nodemailer-express-handlebars": "^3.0.0",
    "nodemon": "^1.18.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.9.0",
    "register-service-worker": "^1.6.2",
    "sequelize": "^5.7.0",
    "sequelize-cli": "^5.4.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.0.2",
    "vue2-leaflet": "^2.0.3",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-pwa": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.6.3",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^3.1.1",
    "prettier": "^1.16.4",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
}
